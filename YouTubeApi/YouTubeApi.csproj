<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <PackageId>YouTubeApi</PackageId>
    <Authors>Erik Bierbrauer</Authors>
    <Copyright>Copyright (C) Erik Bierbrauer</Copyright>
    <Version>2.0.0</Version>
    <Description>YouTubeApi is a lightweight music-focused client interface for the internal API's of YouTube and YouTube Music.</Description>
    <PackageTags>youtube music video playlist api</PackageTags>
    <PackageReadMeFile>README.md</PackageReadMeFile>
    <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
    <PackageProjectUrl>https://github.com/ebier92/YouTubeApi</PackageProjectUrl>
    <PackageReleaseNotes>https://github.com/ebier92/YouTubeApi/releases</PackageReleaseNotes>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\README.md" Pack="true" PackagePath="\"/>
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE.txt" Pack="true" PackagePath="\"/>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>

</Project>
